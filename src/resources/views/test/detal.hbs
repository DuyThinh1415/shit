<div class="mt-4">
    <div class="row">
        {{#each foods}}
        <div class="col-sm-6 col-lg-4">
            <div class="card">
                <img src="{{this.image}}" class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <p class="card-text">{{this.describle}}</p>
                    <a href="http://localhost:3000/test/food-list/{{this.slug}}" class="btn btn-primary">Detal</a>
                </div>
            </div>

        </div>
        {{/each}}
    </div>
</div>